{
  "name": "@shapeshiftoss/hdwallet-react",
  "version": "0.1.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "scripts": {
    "build": "microbundle --name=core",
    "build:browser": "microbundle --target=web --external=none --name=core",
    "clean": "rm -rf dist .rts2_*",
    "dev": "microbundle watch --name=core",
    "dev:browser": "microbundle watch --target=web --external=none --name=core",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "dependencies": {
    "@shapeshiftoss/hdwallet-core": "^0.1.0",
    "@shapeshiftoss/hdwallet-keepkey": "^0.1.0",
    "@shapeshiftoss/hdwallet-keepkey-webusb": "^0.1.0",
    "@shapeshiftoss/hdwallet-trezor": "^0.1.0",
    "@shapeshiftoss/hdwallet-trezor-connect": "^0.1.0",
    "hoist-non-react-statics": "^3.3.0",
    "ts-invariant": "^0.4.4"
  },
  "devDependencies": {
    "microbundle": "^0.11.0"
  }
}
